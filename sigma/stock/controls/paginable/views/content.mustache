<table class="table table-hover">
	<thead>
		<tr>
			<th>#</th>
			<th>Nombre</th>
			<th>Apellido</th>
			<th>Email</th>
			<th>Catter</th>
		</tr>
	</thead>
	<tbody>
	{{#collection}}
		<tr>
			<td>
				{{id}}
			</td>
			<td>
				{{nombre}}
			</td>
			<td>
				{{apellido}}
			</td>
			<td>
				{{email}}
			</td>
			<td>
				{{catter}}
			</td>
		</tr>
	{{/collection}}
	</tbody>
</table>	